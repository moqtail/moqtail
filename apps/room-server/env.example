# MOQtail Room Server Environment Variables
# Copy this file to .env and update the values

# Admin password for the room management interface
# Set this to a strong password to enable the admin interface
# The admin interface will be completely disabled if this is not set
MOQTAIL_ADMIN_PASSWORD=your-secure-password-here

# Other optional environment variables
# MOQTAIL_WS_PORT=3001
# MOQTAIL_SECURE_WS=false

# Proxy configuration
# Set to true if running behind a reverse proxy (nginx, etc.)
# MOQTAIL_BEHIND_PROXY=false
# Trust proxy headers for real IP addresses
# MOQTAIL_TRUST_PROXY=false 