{
  "name": "moqtail-ts",
  "version": "0.3.0",
  "description": "Media Over QUIC Transport client implementation",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "moqtail",
    "moq",
    "media",
    "quic",
    "transport",
    "streaming"
  ],
  "homepage": "https://moqtail.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streaming-university/moqtail.git",
    "directory": "libs/moqtail-ts"
  },
  "bugs": {
    "url": "https://github.com/streaming-university/moqtail/issues"
  },
  "author": {
    "name": "Streaming University",
    "email": "info@moqtail.dev",
    "url": "https://moqtail.dev"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "format": "prettier --check .",
    "test:run": "vitest run",
    "clean": "rimraf dist",
    "docs": "typedoc --out docs ./src"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.10",
    "typescript": "^5.1.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "heap-js": "^2.6.0"
  }
}
